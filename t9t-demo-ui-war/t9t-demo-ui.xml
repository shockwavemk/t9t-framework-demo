<?xml version="1.0"?>
<!DOCTYPE Configure PUBLIC "-//Mort Bay Consulting//DTD Configure//EN" "http://www.eclipse.org/jetty/configure_9_3.dtd">

<Configure id='wac' class="org.eclipse.jetty.webapp.WebAppContext">
  <Set name="contextPath">/demo</Set>
  <Set name="war"><Property name="jetty.webapps" default="."/>/t9t-demo-ui.war</Set>
  <Set name="extractWAR">true</Set>
  <Set name="copyWebDir">false</Set>
  <Set name="configurationDiscovered">true</Set>

  <!-- Configure remote connection via JNDI java:comp/env -->
  <New class="org.eclipse.jetty.plus.jndi.EnvEntry">
    <Arg></Arg>
    <Arg>t9t/port</Arg>
    <Arg type="java.lang.String">8825</Arg>
    <Arg type="boolean">false</Arg>
  </New>
  <New class="org.eclipse.jetty.plus.jndi.EnvEntry">
    <Arg></Arg>
    <Arg>t9t/host</Arg>
    <Arg type="java.lang.String">ci.fortytwo.martin-kramer.com</Arg>
    <Arg type="boolean">false</Arg>
  </New>
</Configure>
